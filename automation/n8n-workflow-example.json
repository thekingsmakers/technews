    {
      "parameters": {
        "functionCode": "const normalizedItems = items.map(item => {
  return {
    title: item.json.title,
    summary: item.json.description,
    content: item.json.content,
    category: 'Technology',
    tags: ['news', 'tech'],
    imageUrl: item.json.urlToImage,
    publishedAt: item.json.publishedAt,
    featured: false
  };
});

return normalizedItems;"
      },
      "name": "Normalize Data",
      "type": "n8n-nodes-base.function",
